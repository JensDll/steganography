{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host --mode=development",
    "build": "pnpm run typecheck && vite build --mode=production",
    "build:Debug": "vite build --mode=development",
    "build:Release": "vite build --mode=production",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0",
    "lint:no-console": "eslint dist --parser-options=ecmaVersion:latest,sourceType:module --rule \"no-console: [\"error\", { allow: [\"warn\", \"error\"] }]\" --no-eslintrc --max-warnings 0",
    "format": "prettier . --write && pnpm run lint --fix"
  },
  "dependencies": {
    "@iconify-json/heroicons": "1.1.15",
    "@iconify-json/mdi": "1.1.58",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@types/node": "20.10.2",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitejs/plugin-vue": "4.5.1",
    "@vueuse/core": "10.6.1",
    "autoprefixer": "10.4.16",
    "esbuild": "0.19.8",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-vue": "9.19.2",
    "postcss": "8.4.32",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "tailwindcss": "3.3.5",
    "tailwindcss-plugin-icons": "2.2.0",
    "typescript": "5.3.2",
    "unplugin-auto-import": "0.17.1",
    "unplugin-vue-components": "0.25.2",
    "validierung": "1.0.4",
    "vite": "5.0.4",
    "vue": "3.3.9",
    "vue-eslint-parser": "9.3.2",
    "vue-router": "4.2.5",
    "vue-tsc": "1.8.24"
  }
}
