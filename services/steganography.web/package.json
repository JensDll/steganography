{
  "private": true,
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host --mode=development",
    "build": "pnpm run typecheck && vite build --mode=production",
    "build:Debug": "vite build --mode=development",
    "build:Release": "vite build --mode=production",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0",
    "lint:no-console": "eslint dist --parser-options=ecmaVersion:latest,sourceType:module --rule \"no-console: [\"error\", { allow: [\"warn\", \"error\"] }]\" --no-eslintrc --max-warnings 0",
    "format": "prettier . --write && pnpm run lint --fix"
  },
  "type": "module",
  "dependencies": {
    "@iconify-json/heroicons": "1.1.10",
    "@iconify-json/mdi": "1.1.52",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "@vitejs/plugin-vue": "4.2.3",
    "@vueuse/core": "10.1.2",
    "autoprefixer": "10.4.14",
    "esbuild": "0.17.19",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-vue": "9.14.1",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "tailwindcss": "3.3.2",
    "tailwindcss-plugin-icons": "2.1.1",
    "typescript": "5.0.4",
    "unplugin-auto-import": "0.16.2",
    "unplugin-vue-components": "0.24.1",
    "validierung": "1.0.4",
    "vite": "4.3.9",
    "vue": "3.3.4",
    "vue-eslint-parser": "9.3.0",
    "vue-router": "4.2.1",
    "vue-tsc": "1.6.5"
  }
}
