{
  "private": true,
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host --mode=development",
    "build": "pnpm run typecheck && vite build --mode=production",
    "build:Debug": "vite build --mode=development",
    "build:Release": "vite build --mode=production",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0",
    "lint:no-console": "eslint dist --parser-options=ecmaVersion:latest,sourceType:module --rule \"no-console: [\"error\", { allow: [\"warn\", \"error\"] }]\" --no-eslintrc --max-warnings 0",
    "format": "prettier . --write && pnpm run lint --fix"
  },
  "type": "module",
  "dependencies": {
    "@iconify-json/heroicons": "1.1.13",
    "@iconify-json/mdi": "1.1.54",
    "@tailwindcss/forms": "0.5.6",
    "@tailwindcss/typography": "0.5.10",
    "@types/node": "20.8.8",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "@vitejs/plugin-vue": "4.4.0",
    "@vueuse/core": "10.5.0",
    "autoprefixer": "10.4.16",
    "esbuild": "0.19.5",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-vue": "9.18.0",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "tailwindcss": "3.3.3",
    "tailwindcss-plugin-icons": "2.2.0",
    "typescript": "5.2.2",
    "unplugin-auto-import": "0.16.6",
    "unplugin-vue-components": "0.25.2",
    "validierung": "1.0.4",
    "vite": "4.5.0",
    "vue": "3.3.6",
    "vue-eslint-parser": "9.3.2",
    "vue-router": "4.2.5",
    "vue-tsc": "1.8.20"
  }
}
