{
  "private": true,
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host --mode=development",
    "build": "vite build --mode=production && prettier dist/index.html --write",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0",
    "lint:no-console": "eslint dist --parser-options=ecmaVersion:latest,sourceType:module --rule \"no-console: [\"error\", { allow: [\"warn\", \"error\"] }]\" --no-eslintrc --max-warnings 0",
    "format": "prettier . --write && pnpm run lint --fix"
  },
  "dependencies": {
    "@iconify-json/heroicons": "1.1.8",
    "@iconify-json/mdi": "1.1.45",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vueuse/core": "9.12.0",
    "autoprefixer": "10.4.13",
    "esbuild": "0.17.5",
    "esbuild-register": "3.4.2",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-vue": "9.9.0",
    "postcss": "8.4.21",
    "prettier": "2.8.3",
    "prettier-plugin-tailwindcss": "0.2.2",
    "tailwindcss": "3.2.4",
    "tailwindcss-plugin-icons": "2.1.0",
    "typescript": "4.9.5",
    "unplugin-auto-import": "0.13.0",
    "unplugin-vue-components": "0.23.0",
    "validierung": "1.0.3",
    "vite": "4.1.1",
    "vue": "3.2.47",
    "vue-eslint-parser": "9.1.0",
    "vue-router": "4.1.6",
    "vue-tsc": "1.0.24"
  }
}
