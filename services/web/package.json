{
  "private": true,
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "test": "vitest",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0 --fix",
    "format": "prettier --write .",
    "fix": "pnpm run format && pnpm run lint"
  },
  "dependencies": {
    "@iconify-json/heroicons-outline": "1.1.2",
    "@iconify-json/heroicons-solid": "1.1.2",
    "@iconify-json/mdi": "1.1.29",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.4",
    "@types/node": "18.6.1",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "@vitejs/plugin-vue": "3.0.1",
    "@vueuse/core": "9.0.0",
    "autoprefixer": "10.4.7",
    "esbuild": "0.14.50",
    "esbuild-register": "3.3.3",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-vue": "9.3.0",
    "gsap": "3.10.4",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "tailwindcss": "3.1.6",
    "tailwindcss-plugin-icons": "1.0.7",
    "typescript": "4.7.4",
    "unplugin-auto-import": "0.10.1",
    "unplugin-vue-components": "0.21.2",
    "validierung": "1.0.2",
    "vite": "3.0.3",
    "vitest": "0.19.1",
    "vue": "3.2.37",
    "vue-router": "4.1.3",
    "vue-tsc": "0.39.0"
  }
}
