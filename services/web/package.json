{
  "private": true,
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build --mode=production && prettier dist/index.html --write",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0",
    "lint:no-console": "eslint dist --parser-options=ecmaVersion:latest --rule \"no-console: [\"error\", { allow: [\"warn\", \"error\"] }]\" --no-eslintrc --max-warnings 0",
    "format": "prettier . --write && pnpm run lint --fix"
  },
  "dependencies": {
    "@iconify-json/heroicons": "1.1.5",
    "@iconify-json/mdi": "1.1.33",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.7",
    "@types/node": "18.8.4",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "@vitejs/plugin-vue": "3.1.2",
    "@vueuse/core": "9.3.0",
    "autoprefixer": "10.4.12",
    "esbuild": "0.15.10",
    "esbuild-register": "3.3.3",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-vue": "9.6.0",
    "gsap": "3.11.3",
    "postcss": "8.4.17",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "tailwindcss": "3.1.8",
    "tailwindcss-plugin-icons": "2.0.3",
    "typescript": "4.8.4",
    "unplugin-auto-import": "0.11.2",
    "unplugin-vue-components": "0.22.8",
    "validierung": "1.0.3",
    "vite": "3.1.7",
    "vitest": "0.24.1",
    "vue": "3.2.40",
    "vue-eslint-parser": "9.1.0",
    "vue-router": "4.1.5",
    "vue-tsc": "1.0.6"
  }
}
